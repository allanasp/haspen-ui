{
  "name": "@haspen/nuxt",
  "version": "0.1.0",
  "description": "Nuxt 3 module for Haspen UI",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/allanasp/haspen-ui.git",
    "directory": "packages/nuxt"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build:nuxt": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "test": "../../node_modules/.bin/vitest run"
  },
  "dependencies": {
    "@haspen/ui": "workspace:*",
    "@haspen/composables": "workspace:*",
    "@nuxt/kit": "^3.17.2"
  },
  "devDependencies": {
    "@nuxt/eslint": "1.3.0",
    "@nuxt/eslint-config": "^1.3.0",
    "@nuxt/module-builder": "^1.0.1",
    "@nuxt/schema": "^3.17.2",
    "@nuxt/scripts": "0.11.6",
    "@nuxt/test-utils": "^3.18.0",
    "@types/node": "^20.17.45",
    "@unhead/vue": "^2.0.8",
    "changelogen": "^0.6.1",
    "eslint": "^9.26.0",
    "nuxt": "^3.17.2"
  }
}
