{
  "plugins": [
    "npm",
    [
      "conventional-commits",
      {
        "preset": "angular"
      }
    ],
    "released",
    "first-time-contributor",
    [
      "upload-assets",
      {
        "assets": ["dist/**/*"]
      }
    ],
    [
      "omit-commits",
      {
        "subject": ["^chore", "^ci", "^build", "^style"]
      }
    ]
  ],
  "labels": [
    {
      "name": "major",
      "changelogTitle": "ğŸ’¥ Breaking Changes",
      "description": "Increment the major version when merged",
      "releaseType": "major"
    },
    {
      "name": "minor",
      "changelogTitle": "ğŸš€ Features",
      "description": "Increment the minor version when merged",
      "releaseType": "minor"
    },
    {
      "name": "patch",
      "changelogTitle": "ğŸ› Bug Fixes",
      "description": "Increment the patch version when merged",
      "releaseType": "patch"
    },
    {
      "name": "release",
      "changelogTitle": "ğŸ“¦ Release",
      "description": "Mark this PR for release",
      "releaseType": "none"
    },
    {
      "name": "skip-release",
      "description": "Skip the release process",
      "releaseType": "skip"
    }
  ],
  "baseBranch": "main",
  "prereleaseBranches": ["develop", "next", "canary"],
  "onlyPublishWithReleaseLabel": false
}